UserManager userManager = new UserManager(playerDbDAO, teamDbDAO, config);\r\n        //userManager.deleteAllUsersFromDB();\r\n        TeamManager teamManager = new TeamManager(userManager, teamDbDAO, playerDbDAO, teamRankingDbDAO, leagueDbDAO, matchDbDAO);\r\n        LeagueManager leagueManager = new LeagueManager(leagueDbDAO, teamDbDAO,userManager, calendarDbDAO, teamRankingDbDAO);\r\n\r\n\r\n        League league = new League(\"Liga1\", \"2023/12/12\", \"12:00\", null, null, \"Liga1\", \"resources/leagues/League1.png\");\r\n        //leagueDbDAO.exportLeagueToDB(league);\r\n\r\n        MainView mainView = new MainView();\r\n        MainController mainController = new MainController(mainView);\r\n\r\n        LoginPanel loginPanel = new LoginPanel();\r\n        RecoverPasswordPanel recoverPasswordPanel = new RecoverPasswordPanel();\r\n        LoginView loginView = new LoginView(\"resources/login/fons.png\",loginPanel,recoverPasswordPanel);\r\n\r\n        FileDialog fileDialog = new FileDialog(mainView);\r\n        CreateLeagueDialog createLeagueDialog = new CreateLeagueDialog(mainView);\r\n        AdminMenuView adminMenuView = new AdminMenuView(\"resources/admin_menu/adminFons.png\",fileDialog,createLeagueDialog);\r\n        UserMenuView userMenuView = new UserMenuView(\"resources/player_menu/playerFons.png\");\r\n\r\n        AdminManage adminManage = new AdminManage();\r\n        AdminShowTeamsDelete adminShowTeamsDelete = new AdminShowTeamsDelete();\r\n        AdminShowLeaguesDelete adminShowLeaguesDelete = new AdminShowLeaguesDelete();\r\n        AdminAddTeamsToLeagues adminAddTeamsToLeagues = new AdminAddTeamsToLeagues();\r\n        ViewLeagues viewLeagues = new ViewLeagues();\r\n        AdminOptionView adminOptionView = new AdminOptionView(adminManage,adminShowTeamsDelete, adminShowLeaguesDelete,adminAddTeamsToLeagues,createLeagueDialog,viewLeagues);\r\n\r\n        AdminOptionController adminOptionController = new AdminOptionController(adminOptionView,mainController,teamManager,leagueManager);\r\n        adminOptionView.registerController(adminOptionController);\r\n\r\n        LoginController loginController =new LoginController(loginView,mainController,userManager);\r\n        loginView.registerController(loginController);\r\n\r\n        AdminMenuController adminMenuController = new AdminMenuController(adminMenuView,mainController,teamManager,leagueManager,userManager, adminOptionView);\r\n        adminMenuView.registerController(adminMenuController);\r\n\r\n        UserMenuController userMenuController = new UserMenuController(userMenuView, mainController,teamManager,leagueManager,userManager);\r\n        userMenuView.registerController(userMenuController);\r\n\r\n\r\n        mainView.addCards(loginView,adminMenuView, userMenuView,adminOptionView);\r\n        mainView.start();\r\n\r\n\r\n        //TODO: ELIMINAR testTeamsFiles\r\n        //testTeamsFiles(teamManager);\r\n    }\r\n\r\n    private static void testTeamsFiles(TeamManager teamManager){\r\n        for (int i = 0; i < 20; i++) {\r\n            try {\r\n                teamManager.createTeamFromJSON(\"resources/fitxers_teams/team\"+i+\".json\");\r\n            } catch (TeamAlreadyExistsException | PlayerWithInvalidNumberException | ExistsUserSameDniException |\r\n                     DniNotCorrectException | ExistsUserSameEmailException | EmailNotCorrectException |\r\n                     PlayerNotFoundException | FileNotFoundException ignored) {\r\n\r\n            }\r\n        }\r\n    }\r\n}
