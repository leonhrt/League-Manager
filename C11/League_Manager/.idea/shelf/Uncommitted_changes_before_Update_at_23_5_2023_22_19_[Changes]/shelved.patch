<+>package presentation.controller;\r\n\r\nimport business.model.LeagueManager;\r\nimport business.model.TeamManager;\r\nimport business.model.UserManager;\r\nimport business.model.entities.Player;\r\nimport business.model.exceptions.*;\r\nimport presentation.view.pantalles.AdminMenuView;\r\nimport presentation.view.pantalles.AdminOptionView;\r\nimport presentation.view.pantalles.LoginView;\r\nimport presentation.view.ui_elements.CreateLeagueDialog;\r\nimport presentation.view.ui_elements.FileDialog;\r\n\r\nimport javax.swing.*;\r\nimport java.awt.event.ActionEvent;\r\nimport java.awt.event.ActionListener;\r\nimport java.io.File;\r\nimport java.io.FileNotFoundException;\r\nimport java.util.ArrayList;\r\n\r\npublic class AdminMenuController implements ActionListener {\r\n    private AdminMenuView adminMenuView;\r\n    private MainController mainController;\r\n    private UserManager userManager;\r\n    private TeamManager teamManager;\r\n    private LeagueManager leagueManager;\r\n\r\n    public AdminMenuController(AdminMenuView adminMenuView, MainController mainController, TeamManager teamManager, LeagueManager leagueManager, UserManager userManager) {\r\n        this.adminMenuView = adminMenuView;\r\n        this.mainController = mainController;\r\n        this.teamManager = teamManager;\r\n        this.leagueManager = leagueManager;\r\n        this.userManager = userManager;\r\n    }\r\n\r\n    @Override\r\n    public void actionPerformed(ActionEvent e) {\r\n        switch (e.getActionCommand()) {\r\n            case FileDialog.UPLOAD_FILE_PRESSED -> manageFileDialog();\r\n            case FileDialog.CANCEL_FILE_PRESSED -> mainController.closeFileDialog();\r\n            case AdminMenuView.LOGOUT_PRESSED -> {\r\n                userManager.logoutPlayer();\r\n                mainController.switchView(LoginView.CARD_LOGIN);\r\n                mainController.switchLoginView(\"login\");}\r\n\r\n            case AdminMenuView.CREATE_TEAM_PRESSED ->mainController.askTeamFile();\r\n            case AdminMenuView.MANAGE_PRESSED -> mainController.switchView(AdminOptionView.CARD_ADMIN_OPTION);\r\n            case AdminMenuView.CREATE_LEAGUE_PRESSED ->{\r\n                 adminMenuView.showLeagueForm();\r\n                 adminMenuView.resetLeagueForm();}\r\n\r\n            case AdminMenuView.VIEW_LEAGUES_PRESSED -> System.out.println(\"all leagues\");\r\n            case AdminMenuView.VIEW_MATCHES_PRESSED -> System.out.println(\"matches\");\r\n            case CreateLeagueDialog.NEXT_PRESSED ->\r\n            {\r\n                try {\r\n                    leagueManager.checkLeagueNameAndDate(adminMenuView.getLeagueName(), adminMenuView.getStartDate(), adminMenuView.getStartHour());\r\n                } catch (LeagueNameAlreadyExists | DateAndHourInvalid ex) {\r\n                   adminMenuView.setErrorCreation(ex.getMessage());\r\n                }\r\n            }\r\n            case CreateLeagueDialog.CANCEL_PRESSED ->adminMenuView.closeLeagueForm();\r\n        }\r\n        for (int i = 0; i < adminMenuView.getMatchesSize(); i++) {\r\n            if(e.getActionCommand().equals(AdminMenuView.MATCH_PRESSED+i)){\r\n                System.out.println(e.getActionCommand());\r\n            }\r\n        }\r\n    }\r\n    private void manageFileDialog(){\r\n        int return_value = adminMenuView.askFile();\r\n        if(return_value == JFileChooser.APPROVE_OPTION){\r\n            File file = adminMenuView.getSelectedFile();\r\n            String filename = file.getAbsolutePath();\r\n            try {\r\n                teamManager.createTeamFromJSON(filename);\r\n                adminMenuView.setMessageFile(\"Team was created successfully!\", false);\r\n                ArrayList<Player> new_players = teamManager.getPlayersRegistered();\r\n                if(!new_players.isEmpty()){\r\n                    adminMenuView.showNewPlayers(new_players);\r\n                }\r\n\r\n            } catch (BusinessStoppingException | FileNotFoundException ex) {\r\n                adminMenuView.setMessageFile(ex.getMessage(), true);\r\n            }\r\n        }\r\n        else{\r\n            adminMenuView.setMessageFile(\"You didn't choose a file to upload\", true);\r\n        }\r\n    }\r\n}\r\n\r\n
